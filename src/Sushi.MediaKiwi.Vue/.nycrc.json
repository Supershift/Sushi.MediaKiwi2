{
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "all": true,
  "reporter": ["text", "html", "cobertura", "json"],
  "include": "./src/**/*{.ts,.tsx,.vue}",
  "exclude": ["node_modules", "dist", "cypress", "**/__tests__/**"],
  "report-dir": "./test/component/coverage",
  "temp-dir": "./test/component/.nyc_output"
}
