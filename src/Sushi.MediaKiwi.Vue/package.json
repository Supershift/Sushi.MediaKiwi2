{
  "name": "@supershift/mediakiwi-vue",
  "version": "0.1.3",
  "description": "Vue 3 Library for the MediaKiwi Portal Builder",
  "scripts": {
    "dev": "vue-tsc && vite --config ./vite.config.sample.ts",
    "api": "cd ../Sushi.MediaKiwi.SampleAPI && dotnet build && dotnet watch run",
    "vi": "vitest",
    "vi:coverage": "vitest --coverage",
    "vi:ui": "vitest --ui --api 9999",
    "vi:ui:coverage": "vitest --ui --coverage --api 9999",
    "cy:open": "cypress open",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run:component": "cypress run --component --env coverage=false",
    "cy:coverage:component": "cypress run --component",
    "build:sample": "vue-tsc && vite build --config ./vite.config.sample.ts",
    "preview:sample": "npm run build:sample && vite preview --config ./vite.config.sample.ts",
    "build": "vue-tsc && vite build",
    "update_api": "node ./src/services/generator/swagger-typescript-api.js",
    "update_api_sample": "node ./sample/services/generator/swagger-typescript-api.js"
  },
  "type": "module",
  "files": [
    "dist",
    "lib"
  ],
  "main": "./dist/mediakiwi-vue.umd.js",
  "module": "./dist/mediakiwi-vue.es.js",
  "exports": {
    ".": {
      "require": "./dist/mediakiwi-vue.umd.js",
      "types": "./dist/main.d.ts",
      "import": "./dist/mediakiwi-vue.es.js"
    },
    "./styles": {
      "import": "./dist/mediakiwi-vue.css",
      "require": "./dist/mediakiwi-vue.css"
    },
    "./styles/*": "./lib/styles/*",
    "./styles/abstracts": "./lib/styles/abstracts/index",
    "./styles/base": "./lib/styles/base/index",
    "./styles/components": "./lib/styles/components/index",
    "./styles/layout": "./lib/styles/layout/index",
    "./styles/views": "./lib/styles/views/index"
  },
  "types": "./dist/main.d.ts",
  "dependencies": {
    "@azure/msal-browser": "^2.39.0",
    "@date-io/luxon": "^3.2.0",
    "@vueuse/components": "^12.0.0",
    "@vueuse/core": "^12.0.0",
    "axios": "^1.7.7",
    "i18next": "^25.0.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^3.0.2",
    "luxon": "^3.5.0",
    "material-symbols": "^0.31.6",
    "pinia": "^2.3.1",
    "qs": "^6.13.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mdi/js": "^7.4.47",
    "@pinia/testing": "^0.1.7",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/jsdom": "^21.1.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.9.0",
    "@types/qs": "^6.9.17",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-istanbul": "^3.0.4",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.2.0",
    "eslint": "^9.15.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.31.0",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.81.0",
    "start-server-and-test": "^2.0.8",
    "swagger-typescript-api": "^13.0.23",
    "typescript": "^5.6.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-istanbul": "^7.0.0",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-plugin-vuetify": "^2.1.0",
    "vitest": "^3.0.4",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^2.2.0",
    "vuetify": "3.7.19",
    "webfontloader": "^1.6.28"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Supershift/Sushi.MediaKiwi2.git",
    "directory": "src/Sushi.MediaKiwi.Vue"
  },
  "author": {
    "name": "Supershift",
    "url": "https://www.supershift.nl/"
  },
  "license": "MIT",
  "keywords": [
    "mediakiwi",
    "ui framework",
    "portal",
    "builder",
    "vue",
    "vuetify"
  ],
  "bugs": {
    "url": "https://github.com/Supershift/Sushi.MediaKiwi2/issues"
  },
  "homepage": "https://github.com/Supershift/Sushi.MediaKiwi2#readme"
}
