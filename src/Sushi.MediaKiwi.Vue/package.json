{
  "name": "@supershift/mediakiwi-vue",
  "version": "0.0.1",
  "description": "Vue 3 Library for the MediaKiwi Portal Builder",
  "scripts": {
    "build": "vite build --mode development",
    "build:watch": "vite build --mode development --watch",
    "build:prd": "vite build --mode production",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test": "jest src",
    "test:ui": "vitest --ui",
    "lint:all": "eslint src",
    "lint:error": "eslint src --quiet"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Supershift/Sushi.MediaKiwi2.git",
    "directory": "src/Sushi.MediaKiwi.Vue"
  },
  "license": "MIT",
  "keywords": [
    "mediakiwi",
    "ui framework",
    "portal",
    "builder",
    "vue",
    "vuetify"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/mediakiwi-vue.umd.js",
  "module": "./dist/mediakiwi-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/mediakiwi-vue.es.js",
      "require": "./dist/mediakiwi-vue.umd.js"
    },
    "./dist/mediakiwi-vue.css": {
      "import": "./dist/mediakiwi-vue.css",
      "require": "./dist/mediakiwi-vue.css"
    }
  },
  "types": "./dist/main.d.ts",
  "dependencies": {
    "@azure/msal-browser": "^2.33.0",
    "@vuepic/vue-datepicker": "^4.2.0",
    "axios": "^1.3.2",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.1",
    "pinia": "^2.0.33",
    "reflect-metadata": "^0.1.13",
    "rollup-plugin-typescript2": "^0.34.1",
    "tsyringe": "^4.7.0",
    "vite-plugin-dts": "^2.0.2",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.3"
  },
  "devDependencies": {
    "@mdi/font": "^7.1.96",
    "@pinia/testing": "^0.0.15",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.13.0",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.28.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.14.1",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.7.4",
    "vite": "^4.2.3",
    "vitest": "^0.25.6",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5",
    "vuetify": "^3.1.3",
    "webfontloader": "^1.6.28"
  }
}
