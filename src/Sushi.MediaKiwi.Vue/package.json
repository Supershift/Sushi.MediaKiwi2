{
  "name": "@supershift/mediakiwi-vue",
  "version": "0.1.3",
  "description": "Vue 3 Library for the MediaKiwi Portal Builder",
  "scripts": {
    "dev": "vue-tsc && vite --config ./vite.config.sample.ts",
    "api": "cd ../Sushi.MediaKiwi.SampleAPI && dotnet build && dotnet watch run",
    "vi": "vitest",
    "vi:coverage": "vitest --coverage",
    "vi:ui": "vitest --ui",
    "vi:ui:coverage": "vitest --ui --coverage --api 9999",
    "cy:open": "cypress open",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run:component": "cypress run --component --env coverage=false",
    "cy:coverage:component": "cypress run --component",
    "build:sample": "vue-tsc && vite build --config ./vite.config.sample.ts",
    "preview:sample": "npm run build:sample && vite preview --config ./vite.config.sample.ts",
    "build": "vue-tsc && vite build"
  },
  "type": "module",
  "files": [
    "dist",
    "lib"
  ],
  "main": "./dist/mediakiwi-vue.umd.js",
  "module": "./dist/mediakiwi-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/mediakiwi-vue.es.js",
      "require": "./dist/mediakiwi-vue.umd.js",
      "types": "./dist/main.d.ts"
    },
    "./styles": {
      "import": "./dist/mediakiwi-vue.css",
      "require": "./dist/mediakiwi-vue.css"
    },
    "./styles/*": "./lib/styles/*",
    "./styles/abstracts": "./lib/styles/abstracts/index",
    "./styles/base": "./lib/styles/base/index",
    "./styles/components": "./lib/styles/components/index",
    "./styles/layout": "./lib/styles/layout/index",
    "./styles/views": "./lib/styles/views/index"
  },
  "types": "./dist/main.d.ts",
  "dependencies": {
    "@azure/msal-browser": "^2.33.0",
    "@date-io/dayjs": "^2.17.0",
    "axios": "^1.7.4",
    "dayjs": "^1.11.10",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.1",
    "istanbul-lib-coverage": "^3.2.0",
    "pinia": "^2.0.33",
    "qs": "^6.12.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mdi/js": "^7.2.96",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.18.1",
    "@types/qs": "^6.9.12",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^13.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.14.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.63.6",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.5.5",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-istanbul": "^5.0.0",
    "vite-plugin-static-copy": "^0.17.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.34.6",
    "vue": "^3.4.21",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.5",
    "vuetify": "^3.6.10",
    "webfontloader": "^1.6.28"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Supershift/Sushi.MediaKiwi2.git",
    "directory": "src/Sushi.MediaKiwi.Vue"
  },
  "author": {
    "name": "Supershift",
    "url": "https://www.supershift.nl/"
  },
  "license": "MIT",
  "keywords": [
    "mediakiwi",
    "ui framework",
    "portal",
    "builder",
    "vue",
    "vuetify"
  ]
}
