{
  "name": "@supershift/mediakiwi-azure-function",
  "version": "0.0.1",
  "description": "Azure Function extension for hosting the MediaKiwi Portal Builder",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:ui": "vitest --ui ",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "coverage:ui": "vitest --ui --coverage"
  },
  "main": "./dist/mediakiwi-azure-function.umd.cjs",
  "module": "./dist/mediakiwi-azure-function.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mediakiwi-azure-function.js",
      "require": "./dist/mediakiwi-azure-function.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@azure/functions": "^3.0.0",
    "@types/node": "16.x",
    "@vitest/coverage-istanbul": "^0.34.5",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",    
    "azure-functions-core-tools": "^4.x",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "vite-plugin-dts": "^3.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Supershift/Sushi.MediaKiwi2.git",
    "directory": "src/Sushi.MediaKiwi.AzureFunction"
  },
  "author": {
    "name": "Supershift",
    "url": "https://www.supershift.nl/"
  },
  "license": "MIT",
  "keywords": [
    "mediakiwi",
    "ui framework",
    "portal",
    "builder",
    "vue",
    "vuetify",
    "azure",
    "azure function",
    "static web app"
  ]
}
