{
  "private": true,
  "scripts": {
    "start:all": "concurrently \"npm run function:start\" \"npm run dev\" \"npm run mediakiwi:test:ui\" \"npm run test:ui\"",
    "start:web": "concurrently \"npm run function:start\" \"npm run dev\"",
    "test:ui:all": "concurrently \"npm run mediakiwi:test:ui\" \"npm run test:ui\"",
    "dev": "vite",
    "function:start": "cd ../Function && npm run start",
    "mediakiwi:test:ui": "cd ../../Sushi.MediaKiwi.Vue && npm run test:ui",
    "build": "vite build",
    "preview": "npm run build && vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test": "jest src",
    "test:ui": "vitest --ui",
    "reports": "vite preview --base __vitest__ --outDir ./html",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dotnet:run:api": "cd ../../Sushi.MediaKiwi.SampleAPI && dotnet build && dotnet watch run"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.13.0",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.28.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^8.0.1",
    "cypress": "^12.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.7.4",
    "vite": "^4.4.9",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.6.5",
    "webfontloader": "^1.6.28"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.33.0",
    "axios": "^1.6.0",
    "pinia": "^2.0.0-rc.10",
    "rollup-plugin-typescript2": "^0.34.1",
    "vite-plugin-dts": "^2.0.2",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.1.6",
    "vuetify": "^3.4.2"
  }
}
